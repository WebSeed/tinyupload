{
  "name": "tinyupload",
  "version": "1.2.1",
  "description": "tinyupload browser",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "test": "standard",
    "lint": "standard",
    "fix": "standard --fix",
    "clean": "rm -rf ./dist ./lib",
    "build": "yarn run clean && yarn cjs && yarn umd && yarn min",
    "cjs": "babel src --out-dir lib && cp src/style.css lib/style.css",
    "umd": "BABEL_ENV=commonjs NODE_ENV=development webpack",
    "min": "BABEL_ENV=commonjs NODE_ENV=production webpack"
  },
  "standard": {
    "ignore": [
      "dist",
      "lib",
      "lambda"
    ]
  },
  "author": "Will Bamford",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.22.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "html-webpack-plugin": "^2.28.0",
    "standard": "^8.6.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-merge": "^2.6.1"
  },
  "dependencies": {
    "tiny-emitter": "^1.1.0"
  }
}
